<h3>Available users</h3>
<ul>
<% @users.each do |user| %>
  <li>
    <%= link_to user.id, user %>
    | <a href="<%= user_path(user) %>" target="_blank"><%= user.username %></a>
    | <%= user.role %>
    | <%=  link_to "Edit", "/users/#{user.id}/edit" %>
  </li>
<% end %>

<%= link_to "Register", new_user_path %>
</ul>